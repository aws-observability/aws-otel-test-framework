{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "structured log schema",
    "description": "json schema for the cloudwatch agent k8s structured log",
    "type": "object",
    "properties": {
      "ClusterName":{},
      "Type":{},
      "NodeName": {},
      "Sources":{},
      "Timestamp":{},
      "Version":{},
      "Namespace":{},
      "namespace_number_of_running_pods":{},
      "_aws": {
        "properties": {
          "CloudWatchMetrics": {},
          "Timestamp": {}
        },
        "required": [
          "CloudWatchMetrics",
          "Timestamp"
        ]
      },
      "kubernetes": {
        "type": "object",
        "properties": {
          "namespace_name": {}
        },
        "required": ["namespace_name"],
        "additionalProperties": false
      }
    },
    "required": [
      "ClusterName",
      "Type",
      "NodeName",
      "Sources",
      "Timestamp",
      "Version",
      "Namespace",
      "kubernetes",
      "namespace_number_of_running_pods",
      "_aws"
    ]
  }